<launch>

    <!-- Add param argument to select new tags-->
    <arg name = "sdfDir" default ="$(find-pkg-share apriltag_gazebo)/models/Apriltag36_11_00000/model.sdf"/>
    <arg name = "offset" default = "0.05"/>

    <executable cmd = "ign gazebo -v 4 empty.sdf" output = "screen"/>

    <node
    pkg = "ros_ign_gazebo"
    exec = "create"
    name = "aprilTag"
    output = "screen"
    args = "-file $(var sdfDir) -z $(var offset)"/>

</launch>